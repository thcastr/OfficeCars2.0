<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/progresso"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.thayn.officecars.andamento">

    <TextView
        android:id="@+id/andamento"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:contentDescription="descrição de andamento"
        android:text="Andamento do serviço"
        android:textSize="24dp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="4dp" />

    <ProgressBar
        android:id="@+id/visualizacao"
        style="?android:attr/progressBarStyle"
        android:layout_width="146dp"
        android:layout_height="133dp"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:id="@+id/atuais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Serviços atuais"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="227dp" />

    <TextView
        android:id="@+id/servico1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Troca de bateria"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="262dp" />

    <TextView
        android:id="@+id/servico2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Limpeza técnica do motor"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="292dp" />

    <TextView
        android:id="@+id/finalizados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Serviços finalizados"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="322dp" />

    <TextView
        android:id="@+id/servico3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Troca de óleo lubrificante"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="360dp" />

    <TextView
        android:id="@+id/servico4"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:text="Limpeza do ar condicionado"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="386dp" />

    <TextView
        android:id="@+id/proximos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Próximo serviço"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="418dp" />

    <TextView
        android:id="@+id/servico5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Polimento"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="451dp" />
</android.support.constraint.ConstraintLayout>
